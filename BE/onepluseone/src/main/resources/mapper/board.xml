<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.onepluseone.onepluseone.mapper.BoardMapper">
	<select id="selectBoard" resultType="BoardDto">
    SELECT id,title,content,nickname,start_date,photo
    FROM board
    ORDER BY id
	</select>

    <insert id="insertBoard" parameterType="BoardDto">
        INSERT INTO board (title,content,photo,password,nickname,start_date)
        VALUES (#{title},#{content},#{photo},#{password},#{nickname},now())
    </insert>
</mapper>